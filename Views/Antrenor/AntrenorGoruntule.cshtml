@model List<WebProjesi.Models.AntrenorModeli>

@{
    ViewData["Title"] = "Antrenörler";
}

<div class="container mt-5">
    <h3 class="text-center mb-4">Antrenör Listesi</h3>

    <table class="table table-striped table-hover shadow">
        <thead class="table-dark">
            <tr>
                <th>#</th>
                <th>Ad Soyad</th>
                <th>Hizmetler</th>
                <th>Müsait Saatler</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var a in Model)
            {
                <tr>
                    <td>@a.Id</td>
                    <td>@a.AdSoyad</td>
                    <td>@a.Hizmetler</td>
                    <td>@a.MusaitSaatler</td>
                </tr>
            }
        </tbody>
    </table>
</div>
